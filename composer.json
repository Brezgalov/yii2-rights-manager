{
  "name": "brezgalov/yii2-rights-manager",
  "description": "Web Interface and tools/actions set for managing yii2 rbac",
  "type": "library",
  "license": "MIT",
  "config": {
    "fxp-asset": {
      "enabled": false
    }
  },
  "authors": [
    {
      "name": "Oleg Brezgalov",
      "email": "oleg.brezgalov@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "yiisoft/yii2": "^2.0.45",
    "kartik-v/yii2-grid": "dev-master",
    "kartik-v/yii2-editable": "dev-master",
    "brezgalov/yii2-modules": "^1.0.4",
    "brezgalov/yii2-api-helpers": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "Brezgalov\\RightsManager\\": "src/"
    }
  }
}
